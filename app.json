{
  "expo": {
    "name": "Recosante",
    "slug": "recosante",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "recosante",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3643b6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.recosante.recosante",
      "buildNumber": "32",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["recosante"]
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "appStoreUrl": "https://apps.apple.com/fr/app/id6476136888"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/android_adaptive_icon.png",
        "backgroundColor": "#3643b6"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.recosante.recosante",
      "versionCode": 27,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.recosante.recosante",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "BIND_GET_INSTALL_REFERRER_SERVICE",
        "CHECK_LICENSE",
        "AD_ID"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8d8b446e-c8db-4641-b730-5cef195e96da"
      }
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "owner": "recosante",
    "plugins": [
      [
        "react-native-fbsdk-next",
        {
          "appID": "2091749431177634",
          "clientToken": "336c13eb382494f41b9ea9faf3fccf3a",
          "displayName": "Recosanté",
          "scheme": "fb2091749431177634",
          "advertiserIDCollectionEnabled": true,
          "autoLogAppEventsEnabled": true,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      "sentry-expo",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Nous n'enregistrons pas vos coordonnées mais nous conservons seulement votre code INSEE de commune, afin de vous fournir des indicateurs précis.",
          "locationAlwaysPermission": "Nous n'enregistrons pas vos coordonnées mais nous conservons seulement votre code INSEE de commune, afin de vous fournir des indicateurs précis.",
          "locationWhenInUsePermission": "Nous n'enregistrons pas vos coordonnées mais nous conservons seulement votre code INSEE de commune, afin de vous fournir des indicateurs précis."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "incubateur",
            "project": "recosante-expo-app",
            "url": "https://sentry.fabrique.social.gouv.fr/"
          }
        }
      ]
    }
  }
}
