{
  "name": "recosante",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "license": "Apache-2.0",
  "private": true,
  "author": "Fabrique des minist√®res sociaux",
  "repository": "git@github.com:SocialGouv/recosante-expo-app.git",
  "scripts": {
    "get-ip": "./get-ip.sh && echo \"this script updates the API IP to your own IP, because that's what Android Emulator needs to work. It's based on your wifi IP, if you need ethernet it won't work (needs en1 instead of en0)\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": " npm run format && npm run ts:check && expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "prepare": "husky install",
    "lint": "eslint  --fix ",
    "lint-check": "eslint --no-fix --max-warnings 0 --no-fix --max-warnings 0",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier src/**/*.ts* --check .",
    "build-eas:android-aab": "yarn prebuild && eas build -p android",
    "build-eas-then-submit:ios": "yarn prebuild && eas build -p ios && eas submit -p ios --latest",
    "build-local:android-aab": "yarn prebuild && eas build -p android --local",
    "build-local:ios-ipa": "yarn prebuild && eas build -p ios --local",
    "build-local:android-apk": "eas build -p android --profile preview --local",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "prebuild": "yarn add patch-package && yarn patch-package && node ./generate-easignore.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.24.3",
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.7",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.21",
    "@types/react": "~18.2.14",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "expo": "^50.0.13",
    "expo-application": "~5.8.3",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-font": "~11.10.3",
    "expo-haptics": "~12.8.1",
    "expo-image": "~1.10.6",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-notifications": "~0.27.6",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-store-review": "~6.8.3",
    "expo-updates": "~0.24.12",
    "fetch-retry": "^5.0.6",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "nativewind": "^2.0.11",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-autocomplete-dropdown": "^3.1.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-pager-view": "6.2.3",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-webview": "13.6.4",
    "tailwind-merge": "^2.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@socialgouv/releaserc": "^1.1.1",
    "@types/babel__helper-module-imports": "^7",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "prettier": "2",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "semantic-release": "^21.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "npm": "please-use-yarn"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "printWidth": 80,
    "tabWidth": 2,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
